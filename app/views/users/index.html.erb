<div class="container">
  <div class="col-md-8">
    <h3>Users</h3>
    <div class="">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>

      <%= paginate @users %>
    </div>
  </div>
  <div class="col-md-4">
      Clips: <%= @clips %>
      <br/>
      Views: <%= @views %>
      <br>
      Plays: <%= @plays %>
      <br>
      Users: <%= @users.count %>
      <br>
      Uploads: <%= @uploads %>
  </div>
</div>
